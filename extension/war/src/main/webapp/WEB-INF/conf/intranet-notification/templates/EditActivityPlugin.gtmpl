<div class="quoteContainer">
  <div class="comment">
  </div>
  <div class="commentNoHtml">
  </div>
  <div class="readMore">
    <%=_ctx.appRes("Notification.label.readMore");%>
  </div>
</div>
<div class="lastUpdatedTime">$LAST_UPDATED_TIME</div>
</div>
        </div>
        </div>
