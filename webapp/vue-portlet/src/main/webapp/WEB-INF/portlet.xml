    </supports>
    <portlet-info>
      <title>Spaces Hamburger Menu</title>
    </portlet-info>
    <portlet-preferences>
      <preference>
        <name>jsModule</name>
        <value>PORTLET/social-vue-portlet/SpacesHamburgerMenu</value>
      </preference>
    </portlet-preferences>
  </portlet>

